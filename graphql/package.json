{
  "name": "GraphQL",
  "version": "1.0.0",
  "description": "GraphQL Apollo server",
  "main": "dist/index.ts",
  "scripts": {
    "dev": "yarn run start:dev & yarn run generate:watch",
    "generate:watch": "graphql-codegen --config codegen.yml --watch",
    "generate": "graphql-codegen --config codegen.yml",
    "build": "npx tsc && cp src/schema/*.graphql dist/schema",
    "lint": "eslint",
    "start:dev": "NODE_ENV=development tsx watch --tsconfig ./tsconfig.json ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "tsx --test src/**/*.test.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/datasource-rest": "^4.3.2",
    "@apollo/server": "^4.1.1",
    "@graphql-tools/schema": "^9.0.12",
    "@types/winston": "^2.4.4",
    "add": "^2.0.6",
    "cors": "^2.8.5",
    "dataloader": "^2.1.0",
    "deepmerge": "^4.2.2",
    "form-data": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-sse": "^2.0.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.11.2",
    "http": "^0.0.1-security",
    "ioredis": "^5.3.1",
    "jose": "^4.11.2",
    "lodash": "^4.17.21",
    "node-forge": "^1.3.1",
    "openid-client": "^5.4.0",
    "semver": "^7.6.2",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "ws": "^8.11.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@apollo/rover": "^0.12.2",
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/typescript": "^2.8.6",
    "@graphql-codegen/typescript-resolvers": "^2.7.11",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.12.13",
    "@types/node-forge": "^1.3.5",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "tsx": "^4.11.0"
  }
}
